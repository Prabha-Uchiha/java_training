import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
   
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        
        int n_square = n*n;
        int rev_n = reverse(n);
        int rev_square =reverse(n_square);
        int rsq = rev_n * rev_n;
        if (rev_square == rsq){
            System.out.println("Adam Number");
        }
        else{
             System.out.println("Not a adam number");
        }
    }
    
    public static int reverse(int n){
        int ld, rev=0;
        while(n>0){
            ld = n%10;
            rev=rev*10 + ld;
            n/=10;
        }
        return rev;
    }
}
