import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        long n = sc.nextLong();
        long rev =0,ld;
        long n1 =n;
        while(n>0){
        ld = n%10;
        rev = rev*10 + ld ;
        n/=10;
    }
        if(n1 == rev)
            System.out.println("Palindrome");
        else
            System.out.println("Not a Palindrome");
          
    }
}
