import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        int MAX =1900;
        int MIN = 9999;
        String date = sc.nextLine();
        String D[] = date.split("/");
        
        int d = Integer.parseInt(D[0]);
        int m =Integer.parseInt(D[1]);
        int y = Integer.parseInt(D[2]);
        boolean valid = false;
        
        
        boolean Leap =false;
        if (((y%4==0) && (y%100!=0)) || (y%400==0))
            Leap=true;
        
    
        if ((y>MAX || y<MIN) || (m<1 || m>12) ||  (d<1 || d>31) )
            valid = false;
            
        if (Leap)
            if(m==2)
                if (d<=29)
                    valid = true;
                else 
                    valid = false;
         
        if((m==1) || (m==3) || (m==5) ||(m==7) || (m==8)|| (m==10) || (m==12))
            if(d<=31)
                valid = true;
            else
                valid = false;
        if ((m==4) || (m==6) || (m==9) || (m==11))
            if(d<=30)
                valid=true;
            else
                valid=false;
                
        if(valid)
            System.out.println("Valid");
        else
            System.out.println("Invalid");
            
        

    }    
}
