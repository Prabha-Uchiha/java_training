import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        
        int g = gcd(a,b);
        int lcm = (a*b)/g;
        System.out.println(lcm);
        
        
    }
    static int gcd(int a,int b){
        if (a==0)
            return b;
        if (b==0)
            return a;
        
        if(a>b)
            return gcd(b,a%b);
        else
            return gcd(a,b%a);
        
        }
}
