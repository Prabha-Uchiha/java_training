import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       
        Scanner sc = new Scanner(System.in);
        int k = sc.nextInt();
        String str = sc.next();
        k = k % str.length();
        System.out.println(str.substring(k)+str.substring(0,k));
        
        
    }
}
