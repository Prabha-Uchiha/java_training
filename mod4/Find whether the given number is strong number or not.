import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int fact =1,ld,sum=0;
        int cpy = n;
        while(n>0){
            ld = n%10;
            n/=10;
            fact =1;
        for(int i=1;i<=ld;i++){
            fact = fact*i;
        }
        
        sum+=fact;}
        
        if (cpy == sum){
            System.out.println("Strong Number");
        }
        else
            System.out.println("Not a strong number");
            
            
    }
}
